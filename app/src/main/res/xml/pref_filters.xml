<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:defaultValue="V"
        android:dialogTitle="@string/log_level"
        android:entries="@array/levels_entries"
        android:entryValues="@array/levels_values"
        android:key="@string/pref_log_level"
        android:title="@string/log_level" />
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="@string/pref_app_filter"
        android:title="@string/app_filter"
        android:summaryOn="@string/auto_filter_summary"
        android:summaryOff="@string/no_app_filter"/>
    <Preference
        android:key="@string/pref_tag_filter"
        android:title="@string/tag_filter" />
    <Preference
        android:key="@string/pref_key_filter"
        android:title="@string/key_filter" />



</PreferenceScreen>
